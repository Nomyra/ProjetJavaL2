                                <Pane fx:id="row0col2" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="0" />
                                <Pane fx:id="row1col0" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                                <Pane fx:id="row1col1" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                <Pane fx:id="row1col2" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                                <Pane fx:id="row2col0" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                                <Pane fx:id="row2col1" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                <Pane fx:id="row2col2" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                            </children>
                        </GridPane>
                        <Pane fx:id="resultatPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="50.0">
                            <HBox.margin>
                                <Insets left="100.0" top="90.0" />
                            </HBox.margin>
                        </Pane>
                        <Label fx:id="nbItem" textAlignment="RIGHT" textFill="RED">
                            <font>
                                <Font size="24.0" />
                            </font>
                            <HBox.margin>
                                <Insets top="71.0" />
                            </HBox.margin>
                        </Label>
                        <Button fx:id="enregistrer" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" text="Enregistrer" BorderPane.alignment="TOP_RIGHT">
                            <cursor>
                                <Cursor fx:constant="HAND" />
                            </cursor>
                        </Button>
                        <Button fx:id="menu" mnemonicParsing="false" text="Menu" />
                    </children>
                    <BorderPane.margin>
                        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
                    </BorderPane.margin>
                </HBox>
            </top>
            <center>
                <VBox fx:id="vboxVariable" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="45.0" styleClass="hinventaire" text="Inventaire" />
                        <ScrollPane minHeight="200" minWidth="842" prefHeight="200.0">
                            <FlowPane fx:id="inventairePane" minHeight="200" minWidth="842" />
                        </ScrollPane>
                    </children>
                </VBox>
            </center>
            <left>
                <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label text="Aide" />
                        <MenuButton mnemonicParsing="false" text="MenuButton">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Action 1" />
                                <MenuItem mnemonicParsing="false" text="Action 2" />
                            </items>
                        </MenuButton>
                    </children>
                </VBox>
